<%- include("../partials/head.ejs") %>
<body>
  <%- include("../partials/header.ejs") %>
<body>
  
  <main class="form-main">
  </nav>
  <section class="form_main_section">
    <h1>CARGAR PRODUCTOS</h1>
    <form action="form" method="post" class="form_mainsection_form">
      <div>
        <label for="imagen">Imagen</label><br>
        <input type="file" accept="png jpg">
      </div>
        <div>
            <label for="name">Nombre</label><br>
            <input type="text" name="nombre" id="name" required>
        </div>
    
          <div>
            <label for="price">Precio</label><br>
            <input type="price" name="price" id="price" required>
          </div>
  

          <div>
            <label for="description">Descripción</label><br>
            <input type="description" name="description" id="description" required>
          </div>
  
          <div>
            <label for="descriptions a">Descripción Adicional</label><br>
            <input type="descriptions a" name="descriptions a" id="descriptions a" required>
          </div>
          <div>
            <label for="measures">Medidas</label><br>
            <input type="measures" name="measures" id="measures" required>
          </div>
          
          <button class="form-button--a a"><a href="http://localhost:3000">Cargar Producto</a></button> 
          
    </form>
    
  </section>
  
   </main>
   <%- include("../partials/footer.ejs") %>

</body>
</html>